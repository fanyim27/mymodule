  print(avg_value)


def mode(n_num):
  n_num = []
  n = len(n_num)
  data = Counter(n_num)
  get_mode = dict(data)
  mode = [k for k, v in get_mode.items() if v == max(list(data.values()))]
  
  if len(mode) == n:
    get_mode = "No mode found"
  else:
    get_mode = "Mode is / are: " + ', '.join(map(str, mode))
      

def cubenumber(number):
  cube = number * number * number
  return(cube)
